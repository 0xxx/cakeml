INCLUDES = ../../bytecode $(HOLDIR)/examples/set-theory/hol_sets
OPTIONS = QUIT_ON_FAILURE

ifdef POLY
HOLHEAP = heap
EXTRA_CLEANS = $(HOLHEAP) $(HOLHEAP).o

BARE_THYS = $(HOLDIR)/examples/set-theory/hol_sets/cardinalTheory ../../bytecode/miscTheory

DEPS = $(patsubst %,%.uo,$(BARE_THYS))

$(HOLHEAP): $(DEPS)
	$(protect $(HOLDIR)/bin/buildheap) -o $(HOLHEAP) $(BARE_THYS)
endif
