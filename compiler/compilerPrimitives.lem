open Lib
open Ast
open SemanticPrimitives
open CompilerLib

let rec
map_result f (Rval v) = Rval (f v)
and
map_result _ (Rerr e) = Rerr e

let rec
every_result _ (Rerr _) = true
and
every_result P (Rval v) = P v

let rec
num_to_bool 0 = false
and
num_to_bool _ = true

let rec
is_Short (Short _) = true
and
is_Short _ = false
